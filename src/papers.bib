@article{melnikova_nanosatellite_2017,
	title = {Nanosatellite aerobrake maneuvering device},
	rights = {All rights reserved},
	url = {https://arxiv.org/abs/1708.07417},
	abstract = {In this paper, we present the project of the solar sail unit for deployment of {CubeSat} constellation. It is also possible to use this unit for de-orbiting of satellites in fully passive mode. We prove the feasibility of the concept by ballistic calculations taking into account dependence of time of constellation formation from the number of satellites and orbital altitude. For ballistic calculations, we utilized the {GMAT} software as well as newly written software to optimize the constellation deployment sequence. We calculated the time needed to deploy a constellation of 2, 4 or 8 3U {CubeSats} on the initial orbit with {ISS} orbit inclination and with initial altitude ranges from 450 to 650 km with different drag area. This period can vary from 0.18 years for 450km initial orbit and 2 {CubeSats} up to 1.4 years for 650km initial orbit and 8 {CubeSats}. We describe the technical characteristics of the unit, including its structural and electrical design, and consider several problems such as aspects of its integration into a standard {CubeSat} frame. The outer dimensions of the unit are 96x96x46 mm, and it can be integrated into the typical {CubeSat}. We also analyzed several economic and social benefits, prospects, and advantages of using the solar sail unit with the other propulsion systems.},
	journaltitle = {{arXiv}:1708.07417 [astro-ph]},
	author = {Melnikova, Valeria G. and Borovikov, Alexander A. and Koretskii, Maksim I. and Smirnova, Yuliya L. and Timakova, Ekaterina D. and Yu, Zhaokai and Kuznetsov, Arseniy O. and Frolov, Kirill A. and Tenenbaum, Stepan M. and Rachkin, Dmitriy A. and Kotsur, Oleg S. and Nerovny, Nikolay A. and Mayorova, Vera I. and Grigorjev, Anton and Goncharov, Nikita},
	date = {2017-08-24},
	eprinttype = {arxiv},
	eprint = {1708.07417},
	keywords = {Astrophysics - Instrumentation and Methods for Astrophysics},
	file = {arXiv\:1708.07417 PDF:/home/locutus/Zotero/storage/4T5ASWX7/Melnikova et al. - 2017 - Nanosatellite aerobrake maneuvering device.pdf:application/pdf;arXiv.org Snapshot:/home/locutus/Zotero/storage/ER3CPJ2E/1708.html:text/html},
}

